<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="517.0" prefWidth="608.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/20.0.1" fx:controller="com.example.cft_testtask.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <TableView fx:id="tblView" prefHeight="200.0" prefWidth="200.0">
     <columns>
       <TableColumn fx:id="idCol" prefWidth="75.0" text="idCol" />
       <TableColumn fx:id="nameCol" prefWidth="75.0" text="nameCol" />
         <TableColumn fx:id="surnameCol" prefWidth="75.0" text="surnameCol" />
         <TableColumn fx:id="patronymicCol" prefWidth="75.0" text="patronymic" />
     </columns>
      <columnResizePolicy>
         <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
      </columnResizePolicy>
   </TableView>

    <Label fx:id="welcomeText" />
</VBox>
